#cat
  ul.nosl.reset
    each item in data.items
      li.fix-cover
        .overlay
          .actions
            +linkFor(`/${item.product.key}/`, 'View more').solid-shadow.button
            button.solid-shadow(data-buy=item.product.key) $#{item.product.price} MXN &mdash; SHOP NOW
        img(alt=item.product.name src=item.product.image)
